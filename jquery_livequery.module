<?php

/**
 * Implements hook_library().
 */
function jquery_livequery_library() {
  $libraries = array();

  $libraries['livequery'] = array(
    'title' => 'Live Query',
    'version' => '1.3.6',
    'js' => array(
      drupal_get_path('module', 'jquery_livequery') . '/js/jquery.livequery.min.js' => array(),
    ),
  );

  return $libraries;
}